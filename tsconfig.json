{
  "compilerOptions": {
    "noEmit": true,
    "module": "node16",
    "target": "es2018",
    "lib": ["es2018"],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["lib/**/*.js", "plugins/**/*", "test/cli/**/*"],
  "exclude": [
    "lib/svgo-node.js",
    "lib/svgo-node.test.js",
    "lib/svgo.js",
    "lib/builtin.js",
    "lib/svgo.test.js",
    "lib/svgo/**/*.js",
    "plugins/plugins.js",
    "plugins/preset-default.js"
  ]
}
